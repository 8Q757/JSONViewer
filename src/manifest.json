{
  "name": "JSONViewer",
  "version": "1.0.4",
  "description": "同时格式化多条JSON字符串，在一个页面最大支持6个编辑器，取决于显示器宽度。",
  "icons": {
    "16": "images/16.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "browser_action": {
    "default_icon": "images/16.png",
    "default_title": "JSONViewer"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "offline_enabled": true,
  "manifest_version": 2,
  "update_url": "http://upext.chrome.360.cn/intf.php?method=ExtUpdate.query"
}